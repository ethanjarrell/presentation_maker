{{#imagepeoples}}

<img src="{{#image_data}}{{image}}{{/image_data}}"/>

<form class="form2" action="/api/scriptures/people" method="post">

<label>Category: People</label>
<label>What scripture could this picture reference?</label>
<select name="image_data[standard_work_location]">
  <option value="book of mormon">Book of Mormon
  </option>
  <option value="new testament">New Testament
  </option>
  <option value="old testament">Old Testament
  </option>
  <option value="pearl of great price">Pearl of Great Price"
  </option>
  <option value="D&C">Doctrine and Covenants
  </option>
</select>
<label>What's the book name and verse number?</label>
<input name="book_and_verse_number" />
<label>What's the verse text?</label>
<textarea name="verse_text">Verse Text</textarea>

<input class="submit" type="submit" value="submit">
</form>
{{/imagepeoples}}
